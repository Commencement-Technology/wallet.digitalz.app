    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
